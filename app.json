{
  "expo": {
    "name": "Bike & Bed Global",
    "slug": "bike-bed-global",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/image.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/image.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/image.png",
      "bundler": "metro"
    },
    "scheme": "bike-bed-global"
  }
}